{
  "version": "2",
  "templates": [
    {
      "categories": [
        "performance",
        "networking"
      ],
      "env": [
        {
          "default": "3.9-r1-hc",
          "label": "Docker image tag",
          "name": "IPERF_TAG"
        }
      ],
      "description": "Cross-platform tool for network performance measurement and tuning",
      "note": "Requires Traefik with iperftcp and iperfudp entrypoints set up. Based on mlabbe/iperf3 (MIT license).",
      "platform": "linux",
      "repository": {
        "stackfile": "stacks/iperf/docker-stack.yml",
        "url": "https://github.com/lzpreslav/portainer-stacks"
      },
      "title": "iperf3 server",
      "type": 3
    },
    {
      "categories": [
        "networking"
      ],
      "env": [
        {
          "label": "Zerotier network to connect to",
          "name": "ZEROTIER_NETWORK"
        },
        {
          "default": "latest",
          "label": "Docker image tag",
          "name": "ZEROTIER_TAG"
        }
      ],
      "description": "Zerotier client",
      "logo": "https://upload.wikimedia.org/wikipedia/commons/f/f1/ZeroTier_Logo.png",
      "note": "Uses the host network and shares the tun iface",
      "platform": "linux",
      "repository": {
        "stackfile": "stacks/zerotier/docker-stack.yml",
        "url": "https://github.com/lzpreslav/portainer-stacks"
      },
      "title": "Zerotier",
      "type": 3
    },
    {
      "categories": [
        "media"
      ],
      "env": [
        {
          "default": "nightly",
          "label": "Docker image tag",
          "name": "YTDL_TAG"
        },
        {
          "default": "1000",
          "label": "GID to use the container as",
          "name": "GID"
        },
        {
          "default": "1000",
          "label": "UID to use the container as",
          "name": "UID"
        },
        {
          "label": "Hostname of the YoutubeDL-Material frontend",
          "name": "YTDL_HOSTNAME"
        },
        {
          "default": "",
          "label": "Middlewares to attach to the Traefik proxy",
          "name": "TRAEFIK_MIDDLEWARES"
        },
        {
          "label": "Youtube API key",
          "name": "ytdl_youtube_api_key"
        },
        {
          "default": "appdata",
          "label": "YoutubeDL-Material Appdata volume",
          "name": "APPDATA_VOLUME"
        },
        {
          "default": "audio",
          "label": "Audio downloads volume",
          "name": "AUDIO_VOLUME"
        },
        {
          "default": "video",
          "label": "Video downloads volume",
          "name": "VIDEO_VOLUME"
        },
        {
          "default": "subscriptions",
          "label": "Playlist subscriptions volume",
          "name": "SUBSCRIPTIONS_VOLUME"
        }
      ],
      "description": "Frontend for youtube-dl, used for downloading audio/video from Youtube, Twitch and other media sites.",
      "logo": "https://upload.wikimedia.org/wikipedia/commons/0/09/YouTube_full-color_icon_%282017%29.svg",
      "platform": "linux",
      "repository": {
        "stackfile": "stacks/youtubedl-material/docker-stack.yml",
        "url": "https://github.com/lzpreslav/portainer-stacks"
      },
      "title": "YoutubeDL-Material",
      "type": 3
    }
  ]
}
